#include <strings.h>
#include <stdio.h>

vwl_harmony(in_wrd,out_wrd)
char *in_wrd,*out_wrd;
{
int len,i,j;
int i_case,a_case;
char lst_ch;

i_case=1;
a_case=1;

len = strlen(in_wrd);
lst_ch = in_wrd[len-1];
if((lst_ch == 'i') || (lst_ch == 'u'))
{
j=0;
for(i=0;i<len;i++)
if(isvowel(in_wrd[i])) {j=i; break;}
for(i=0;i<j+1;i++)
out_wrd[i]=in_wrd[i];
out_wrd[len-1]=in_wrd[len-1];
for(i=len-2;i>j;i--)
{
/* manuRulu -> manuRi + lu -> maniRi */
if((lst_ch =='i') && (in_wrd[i] == 'u')) out_wrd[i] = lst_ch;

/* galigina -> galigu -> galugu */

else if((lst_ch =='u') && (in_wrd[i] == 'i') && i_case) {
out_wrd[i] = lst_ch;
/* To stop ceyagaligu -> ceyugulugu */
a_case = 0;
}

else if((lst_ch =='u') && ((in_wrd[i] == 'a') || (in_wrd[i] == 'o')) && i_case && !a_case) {
/* To stop cUpiMcagaligu -> cUpuMcagalugu */
/* To stop purikoVlipu -> purukoVlupu */
out_wrd[i] = in_wrd[i]; /* do not change the vowel after this*/
i_case = 0;
}

/* wiragadaM -> wiragu -> wirugu */
else if((lst_ch =='u') && (in_wrd[i] == 'a') && a_case) {
out_wrd[i] = lst_ch;
i_case = 0;
}

else if((lst_ch =='u') && (in_wrd[i] == 'i') && a_case && !i_case) {
out_wrd[i] = in_wrd[i]; /* do not change the vowel after this*/
a_case = 0;
}

else if((lst_ch =='u') && (in_wrd[i] == 'u')) {
out_wrd[i] = in_wrd[i]; /* do not change the vowel after this*/
a_case = 0;
i_case = 0;
}

else if((lst_ch =='i') && (in_wrd[i] == 'i')) {
out_wrd[i] = in_wrd[i]; /* do not change the vowel after this*/
a_case = 0;
i_case = 0;
}

else out_wrd[i] = in_wrd[i];
}
out_wrd[len] = '\0';
}
else out_wrd[0] = '\0';
}
